{
  "name": "my-plugin",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn dev:main & vite build -c vite.hmr.config.ts & vite -c vite.hmr.config.ts ",
    "dev:main": "cross-env TARGET=main NODE_ENV=development vite build --mode development -w",
    "build": "yarn build:ui && yarn build:main && rm -rf ./dist/assets",
    "build:ui": "cross-env TARGET=ui vite build",
    "build:main": "cross-env TARGET=main vite build"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/qrcode": "^1.5.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "cross-env": "^7.0.3",
    "vite": "^2.8.6",
    "vite-plugin-singlefile": "^0.7.1"
  },
  "dependencies": {
    "@mastergo/plugin-typings": "^1.0.0",
    "qrcode": "^1.5.3",
    "vue": "^3.2.31"
  }
}
